<%- include('../partials/header') %>
<form action="" method="post">
  <h1>EDIT DATA</h1>
  <div class="mb-3 row">
    <label for="Name" class="col-sm-2 col-form-label">Name</label>
    <div class="col-sm-10">
      <input
        type="text"
        value="<%= nama.name %>"
        class="form-control"
        id="Name"
        name="name"
        placeholder="Enter your name"
        
      />
    </div>
  </div>
  
  
  <div class="d-flex">
    <div style="margin-left: -10px;" class="p-2">Member</div>
    <div style="margin-left: 140px;" class="p-2">
   
    <% memberss.forEach((item, index) => { %>
      
      <div class="col">
        <input
        name="projectid"
        class="form-check-input"
        type="checkbox"
        id="flexCheckDefault"
        value="<%= item.userid %>"><%= item.firstname %>
      
      </div>
      <% }) %>
  </div>
</div>
  
<br><br>


  <button type="submit" class="btn btn-primary">Update
  </button>
 
  <a class="btn btn-warning" href="/projects">Back</a>
 

</form>
  
<%- include('../partials/footer') %>
